<section class="section">
	<div class="container">
		<div class="_tabs-container">
			<div class="ui-kit__wrapper">
				<h2 class="section__title">
					Range slider
				</h2>
				<div class="g-tabs">
					<ul class="ui-kit__tabs-menu">
						<li class="ui-kit__tabs-item g-tabs__item g-tabs__item_active" onclick="o2.gTabs.open(this, 1.2)">
							<a class="ui-kit__tabs-link g-tabs__link" href="javascript:void(0);">Visual</a>
						</li>
						<li class="ui-kit__tabs-item g-tabs__item" onclick="o2.gTabs.open(this, 1.3)">
							<a class="g-tabs__link" href="javascript:void(0);">Show code</a>
						</li>
					</ul>
				</div>
			</div>

			<div class="g-tab g-tab_open" data-tab-id="1.2">
				<div class="g-tab__content">
					{% include "/blocks/g-range-slider/g-range-slider.twig"  with {min: 100, max: 10000, start: [1000,7000]} %}
					<br/>
					{% include "/blocks/g-range-slider/g-range-slider.twig"  with {min: 1000, max: 10000, start: [2000]} %}
				</div>
			</div>
			<div class="g-tab" data-tab-id="1.3">
				<div class="g-tab__content">
					<pre class="prettyprint lang-twig ui-kit__pretty">
// промежуток
{{ '{% include "./blocks/g-range-slider/g-range-slider.twig"  with {min: 100, max: 10000, start: [1000,7000]} %}' }}

// одно значение
{{ '{% include "./blocks/g-range-slider/g-range-slider.twig"  with {min: 1000, max: 10000, start: [2000]} %}' }}
					</pre>
				</div>
			</div>
		</div>
	</div>
</section>